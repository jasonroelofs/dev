[color]
	diff = auto
	status = auto
	branch = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
  st = status
  ci = commit
  co = checkout
  dif = diff
  cp = cherry-pick
  unpushed = rev-list HEAD --pretty ^origin/master
  lg = log --graph --pretty=format:'%Cred%h%Creset - %Cblue[%an]%Creset%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
  pullr = pull --rebase
[merge]
  tool = diff3
[repack]
  usedeltabaseoffset = true
[core]
  editor = vim
  pager = "less -FRSX"
[push]
	default = tracking
