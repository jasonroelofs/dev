{
  "codeLens.enable": true,
  "elixir.pathToElixirLS": "/opt/homebrew/bin/elixir-ls",
  "languageserver": {
    "slint": {
      "filetypes": ["slint"],
      "command": "slint-lsp"
    },
    "ruby": {
      "filetypes": ["ruby"],
      "rootPatterns": ["Gemfile"],
      "command": "ruby-lsp"
    }
  }
}
